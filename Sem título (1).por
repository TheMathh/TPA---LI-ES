programa {
  funcao inicio() {
    
    inteiro idade1
    inteiro idade2
    
    escreva ("Qual a idade de Clóvis?: ")
    leia (idade1)
    escreva ("Qual a idade de Joberval: ")
    leia (idade2)

    se (idade1>idade2)
    escreva ("Clóvis é mais velho que Joberval.")
    senao 
    escreva ("Joberval é mais velho que Clóvis.")
  }
}
