programa {
  funcao inicio() {
    
    inteiro idade
    cadeia nome

    escreva ("Digite seu nome: ")
    leia (nome)

    escreva (nome+ ", digite sua idade: ")
    leia (idade)
    
    se (idade>=18)
    escreva (nome+ ", você pode ir ao show do Dj Jeeh FDC.")
    senao 
    escreva (nome+ ", você não pode ir ao show do DJ Jeeh FDC.")
  }
}
